i = 0
while i<6:
    print(i)
    i = i+1
    if i==3:
        break
        
print('________________')


i = 0
while i<6:
    i=i+1
    if i==3:
        continue
    print(i)
    
    
    -----------------------------------------OUTPUT-------------------------------------------
    
    
0
1
2
________________
1
2
4
5
6
